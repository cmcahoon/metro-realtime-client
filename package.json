{
  "name": "metro-realtime-client",
  "version": "0.1.1",
  "description": "Low level client for LA's Metro Realtime APIs.",
  "bin": {
    "metro-realtime-cli": "lib/cli.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "babel src -d lib",
    "test:unit": "mocha test/unit",
    "pretest": "npm run compile && npm run test:unit",
    "test": "echo 'testing complete!'",
    "coverage:unit": "istanbul cover --root src _mocha -- test/unit/ -R spec",
    "prerelease": "npm run test",
    "release": "standard-version",
    "prepublish": "npm run release"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/cmcahoon/metro-realtime-client"
  },
  "author": "cwongprice@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/cmcahoon/metro-realtime-client/issues"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "bunyan": "^1.8.5",
    "commander": "^2.9.0",
    "fluent-interface-builder": "^1.0.1",
    "lodash": "^4.17.2",
    "prettyjson": "^1.2.1",
    "superagent": "^3.1.0",
    "superagent-bluebird-promise": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "rewire": "^2.5.2",
    "sinon": "^1.17.6",
    "standard-version": "^4.0.0",
    "superagent-mocker": "^0.5.2"
  }
}
